{
  "name": "wojsmol/parafia",
  "description": "WordPress template modernization with Rector 2.x",
  "type": "wordpress-theme",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "rector/rector": "^2.2",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Parafia\\": "library/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Parafia\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "rector": "vendor/bin/rector process --config rector.php"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
